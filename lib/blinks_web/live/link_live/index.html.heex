<main class="flex flex-col gap-5">
  <div class="flex justify-between">
    <h1 class="text-2xl font-bold">Links</h1>
    <.link>
      Add link
    </.link>
  </div>

  <div class="flex flex-col gap-4">
    <div :for={link <- @links} class="py-2 px-4 border border-md rounded">
      <h1>{link.title}</h1>
      <a href={link.url} target="_blank">Access &#8594;</a>
    </div>
  </div>
</main>
